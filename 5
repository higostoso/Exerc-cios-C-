using System;

namespace higor_atv_c_
{
	class Program
	{
		public static void Main(string[] args)
		{
		
		            Console.Write("Digite um caractere:");
	             	char palavra = Convert.ToChar(Console.ReadLine());
	         
			Console.WriteLine("O caractere digitado foi:" +  palavra  );
				
				
			
			Console.Write("Press any key to continue . . . ");
			Console.ReadKey(true);
		}
		
	}
}
